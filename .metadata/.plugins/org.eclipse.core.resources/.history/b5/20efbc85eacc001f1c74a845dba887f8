interface IShape
{
  double area();
}



class Circle implements IShape
{
  int x;
  int y;
  int radius;
  String color;
 
  Circle(int x, int y,int radius,String color)
  {
   this.x = x; 
   this.y = y; 
   this.radius = radius; 
   this.color = color; 
  }
  
  
  public double area() {
   return Math.PI * this.radius * this.radius;
  }
  
  
}

class Square implements IShape
{
  int x;
  int y;
  int size;
  String color;
 
  Square(int x, int y, int size, String color)
  {
   this.x = x; 
   this.y = y; 
   this.size = size;
   this.color = color; 
  }
  

  public double area() {
   return this.size * this.size;
  }
  
  
}


