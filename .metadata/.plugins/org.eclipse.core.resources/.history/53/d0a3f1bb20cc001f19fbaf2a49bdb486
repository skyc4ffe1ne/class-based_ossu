interface IAT {}



class Perso implements IAT{
  String name;
  int age ;
  String gender;
  
  
  Perso(String name,int age, String gender){
   this.name = name; 
   this.age = age; 
   this.gender = gender; 
  }

}



class ExamplesAnchestors{
  IAT p4 = new Perso ("Brian", 78, "male");
  IAT p5 = new Perso ("Viktoria", 81, "female");
  
  IAT p2 = new Perso ("Mike", 48, "male");
  IAT p3 = new Perso ("Rihanna", 38, "female");

  IAT p1 = new Perso ("Brian", 18, "male");

 
}


