import tester.*;
interface ILoBook{
 int count();
// double salePrice(int discount);
// ILoBook allBefore(int y);
// ILoBook sortByPrice();
}


class Book {
  String title;
  String author;
  int year;
  double price;
  
  Book(String title, String author, int year, double price){
    this.title = title;
    this.author = author;
    this.year = year;
    this.price = price;
  }
  
  
}

class ConsLoBook implements ILoBook{
  Book first;
  ILoBook rest;
  
  
  ConsLoBook(Book first, ILoBook rest){
   this.first = first; 
   this.rest = rest; 
  }
  

  public int count() 
  {
    return 1 + this.rest.count; 
  }
}



class MtLoBook implements ILoBook{
  MtLoBook(){}
  
  public int count() {
    return 0;
  }
  
}


class ExamplesILoBook{
 ExamplesILoBook(){} 

 ILoBook mtb = new MtLoBook();
 
 Book b1 = new Book("title1","author1",1934,12.45);
 Book b2 = new Book("title2","author2",2009,32.05);
 Book b3 = new Book("title3","author3",2014,22.16);
 
 ILoBook lob1 = new ConsLoBook(b1, new ConsLoBook(b2, new ConsLoBook(b3,mtb)));
 
 boolean testCont(Tester t)
 {
   return t.checkExpect(this.lob1.count(), 3);
 }
 
}


