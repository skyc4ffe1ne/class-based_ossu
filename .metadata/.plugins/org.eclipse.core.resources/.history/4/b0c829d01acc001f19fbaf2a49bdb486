
class Soup{
 String name;
 float price;
 boolean vegetarian;
 
 
 Soup(String name, float price, boolean vegetarian){
   this.name = name;
   this.price = price;
   this.vegetarian = vegetarian;
 }

}


class Salad{
 String name;
 float price;
 boolean vegetarian;
 String dressed;
 
 
Salad(String name, float price, boolean vegetarian,String dressed){
   this.name = name;
   this.price = price;
   this.vegetarian = vegetarian;
   this.dressed = dressed;
 }

}

class Sandwich{
 String name;
 float price;
 String bread;
 String filling_1;  
 String filling_2;  
 
 
 Sandwich(String name, float price, String bread ,String filling_1, String filling_2){
   this.name = name;
   this.price = price;
   this.bread = bread;
   this.filling_1 = filling_1;
   this.filling_2 = filling_2;
 }

}