import tester.*;
class Book{
  String title;
  String author;
  double price; 
  
  Book(String title, String author, double price){
   this.title = title;
   this.author = author;
   this.price = price;
  }
  
  
  double salePrice(int discount ) {
   return this.price - ((this.price * discount) / 100); 
  }
}






class ExampleBook{
  
  Book b1 = new Book("Titolo", "amigo", 60);

  boolean testSalePrice(Tester t) {
    return t.checkExpect(this.b1.salePrice(30),42);
  }
}